pipeline{
agent any

tools {
    maven 'maven'
  }
stages{
      
        stage('Maven Build'){
                    steps{
                        script{
                               try{
                                    sh "mvn -f pom.xml clean install"
                               }
                               catch(all){
                                    echo "Build stage Failed"
                               }
                        }
                    }
                
          }
    }
}
                     
      
